  object qryGetPurchasePrice: TADOQuery [11]
    Connection = dmGlobal.conGlobal
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'ItemId'
        Attributes = [paSigned]
        DataType = ftFMTBcd
        Precision = 18
        Size = 19
        Value = Null
      end>
    SQL.Strings = (
      'select '
      '  a.Price, '
      '  case when not (c.PurchaseInvoiceDate is null) then '
      '    c.PurchaseInvoiceDate '
      '  else '
      '    a.DateModified'
      '  end as PurchaseInvoiceDate'
      'from PurchasePrice a'
      'left join PurchaseInvoiceHd c on a.LastPI = c.PurchaseInvoiceId'
      'where a.ItemId = :ItemId'
      'order by c.PurchaseInvoiceDate desc')
    Left = 232
    Top = 144
  end
