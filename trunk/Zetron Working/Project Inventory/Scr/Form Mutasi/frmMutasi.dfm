  object qryOtherTransactionHd: TUniQuery
    Connection = dmGlobal.conGlobal
    SQL.Strings = (
      'select * '
      'from OtherTransactionHd'
      'where extract(year from OtherTransactionDate) = :Year'
      'and extract(month from OtherTransactionDate) = :Month'
      'and TransactionTypeId in'
      '  (select TransactionTypeId'
      '  from TransactionType'
      '  where StatusTransaction = '#39'2'#39')'
      'order by OtherTransactionId')
    MasterFields = 'othertransactionid'
    BeforeInsert = qryOtherTransactionHdBeforeInsert
    BeforePost = qryOtherTransactionHdBeforePost
    AfterPost = qryOtherTransactionHdAfterPost
    AfterCancel = qryOtherTransactionHdAfterCancel
    OnNewRecord = qryOtherTransactionHdNewRecord
    Left = 136
    Top = 112
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Year'
      end
      item
        DataType = ftUnknown
        Name = 'Month'
      end>
  end
  object qryOtherTransactionDt: TUniQuery
    Connection = dmGlobal.conGlobal
    SQL.Strings = (
      'select *'
      'from OtherTransactionDt'
      'where OtherTransactionId = :OtherTransactionId')
    MasterSource = dsDefault
    BeforeInsert = qryOtherTransactionDtBeforeInsert
    BeforePost = qryOtherTransactionDtBeforePost
    OnNewRecord = qryOtherTransactionDtNewRecord
    Left = 136
    Top = 144
    ParamData = <
      item
