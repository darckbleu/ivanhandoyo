  object qryPrevSalesInvoice: TUniQuery
    Connection = dmGlobal.conReadOnly
    SQL.Strings = (
      'select '
      '  a.*, '
      '  b.CustomerName, b.DepositBalance'
      'from SalesInvoiceHd a'
      'left join Customer b on a.CustomerId = b.CustomerId'
      'where a.SalesInvoiceId = :SalesInvoiceId')
    Left = 440
    Top = 24
    ParamData = <
      item
        DataType = ftString
        Name = 'SalesInvoiceId'
      end>
  end
  object qryInsSalesPayment: TUniQuery
    Connection = dmGlobal.conGlobal
    SQL.Strings = (
      'insert into SalesPayment'
      
        '  (SalesPaymentId, SalesPaymentDate, CustomerId, StatusApprove, ' +
        'EmployeeId)'
      'values'
      
        '  (:SalesPaymentId, :SalesPaymentDate, :CustomerId, 1, :Employee' +
        'Id)')
    Left = 472
    Top = 56
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'SalesPaymentId'
      end
      item
        DataType = ftUnknown
        Name = 'SalesPaymentDate'
      end
      item
        DataType = ftUnknown
        Name = 'CustomerId'
      end
      item
        DataType = ftUnknown
        Name = 'EmployeeId'
      end>
  end
  object qryInsSalesPaymentDt: TUniQuery
    Connection = dmGlobal.conGlobal
    SQL.Strings = (
      'insert into SalesPaymentDt'
      '  (SalesPaymentId, No, SalesInvoiceId, PaymentTypeId, '
      '  Amount, EmployeeId, CurrencyId, Kurs)'
      'values'
      '  (:SalesPaymentId, 1, :SalesInvoiceId, :PaymentTypeId, '
      '  :Amount, :EmployeeId, :CurrencyId, :Kurs)')
    Left = 504
    Top = 56
    ParamData = <
      item
        DataType = ftUnknown
