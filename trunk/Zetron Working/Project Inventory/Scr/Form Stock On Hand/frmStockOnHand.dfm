  object dtsPrevStockOnHandHd: TADODataSet
    Connection = dmGlobal.conGlobal
    CursorType = ctStatic
    CommandText = 
      'select  '#13#10'  a.WarehouseId,'#13#10'  c.WarehouseName,'#13#10'  b.ItemId, b.It' +
      'emName, '#13#10'  e.ItemCategoryName, b.Type, '#13#10'  b.UnitMeasure, d.Min' +
      'StockQty, a.EndQty, '#13#10'  b.ReorderQty'#13#10'from StockOnHand a'#13#10'left j' +
      'oin Item b on a.ItemId = b.ItemId'#13#10'left join Warehouse c on a.Wa' +
      'rehouseId = c.WarehouseId'#13#10'left join ItemMinStock d on (a.ItemId' +
      ' = d.ItemId and a.WarehouseId = d.WarehouseId)'#13#10'left join ItemCa' +
      'tegory e on b.ItemCategoryId = e.ItemCategoryId '#13#10'where a.Wareho' +
      'useId like :WarehouseId'#13#10'and (a.EndQty <> 0)'#13#10' order by a.Wareho' +
      'useId, a.ItemId'
    Parameters = <
      item
        Name = 'WarehouseId'
        Size = -1
        Value = Null
      end>
    Left = 24
    Top = 248
  end
